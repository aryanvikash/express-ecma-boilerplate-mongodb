  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "nodemon": "^2.0.7",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2"
